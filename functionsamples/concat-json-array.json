{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "prefix": {
            "type": "string",
            "defaultValue": "prefix"
        },
        "vmname": {
            "type": "string",
            "defaultValue": "mgmt01"
        },
        "nic01Name": {
            "type": "string",
            "defaultValue": "nic01"
        }
    },
    "resources": [],
    "outputs": {
        "return": {
            "value": "[concat(parameters('vmname')), '-', (parameters(nic01Name))]",
            "type": "string"
    }
   }
}